const bhaskara = (ax2, bx, c) => {
    let retornos = [];
    let delta = (bx ** 2) - (4 * ax2 * c);
    if (delta < 0) {
        return "Delta Ã© negativo";
    }
    let x1 = (-bx + Math.sqrt(delta)) / 2 * ax2;
    let x2 = (-bx - Math.sqrt(delta)) / 2 * ax2;
    retornos.push(x1);
    retornos.push(x2);
    return retornos;
}

console.log(bhaskara(3, -5, 12));
console.log(bhaskara(1, 3, 2));
